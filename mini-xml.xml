<!-- Минимальное сочетание тегов для работы фида -->
<complexes>										<!-- Жилые комплексы-->
	<complex>									<!-- Конкретный комплекс-->
		<id></id>								<!-- id комплекса-->
		<name></name>							<!-- Название комплекса-->
		<latitude></latitude>					<!-- Широта комплекса-->
		<longitude></longitude>					<!-- Долгота комплекса-->
		<address></address>						<!-- Адрес комплекса-->
		<buildings>								<!-- Здания в  комплексе-->
			<building>							<!-- Здание в  комплексе-->
				<id></id>						<!-- id  здания-->
				<flats>							<!-- квартиры  здания-->
					<flat>						<!-- квартира  здания-->
						<flat_id></flat_id>		<!-- id квартиры-->
						<floor></floor>			<!-- какой этаж-->
						<room></room>			<!-- сколько комнат-->
						<plan></plan>			<!-- план квартиры-->
						<price></price>			<!-- цена квартиры-->
						<area></area>			<!-- площадь квартиры-->
					</flat>
				</flats>
			</building>
		</buildings>
		<sales_info>
			<sales_phone></sales_phone>			<!-- телефон менеджера-->
		</sales_info>
	</complex>
</complexes>
